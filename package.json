  },
  "description": "TIDE 2.0 Library Package",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.1",
    "@storybook/vue3-vite": "^7.5.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.4.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^7.5.1",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.0.12"
  },
  "exports": {
    "./components/*": "./src/components/*.vue",
    "./styles": "./src/assets/css/main.css",
    "./types/*": "./src/types/*.ts",
    "./utilities/*": "./src/utilities/*.ts"
  },
  "files": [
    "src/*"
  ],
  "license": "MIT",
  "name": "tide2-design-system",
  "scripts": {
    "build": "run-p type-check build-only && rm -rf dist/",
    "build-only": "vite build",
    "build-storybook": "storybook build",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore --fix",
    "sb": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit"
  },
  "type": "module",
  "version": "0.2.2"
