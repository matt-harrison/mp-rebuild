{
  "author": "Trader Interactive",
  "dependencies": {
    "@storybook/test": "^8.0.9",
    "vue": "^3.2.45"
  },
  "description": "TIDE 2.0 Library Package",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-mdx-gfm": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/vue3": "^8.0.9",
    "@storybook/vue3-vite": "^8.0.9",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.4.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^8.0.9",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.0.12"
  },
  "exports": {
    ".": "./dist/tide2-design-system.js",
    "./css": "./dist/style.css",
    "./css/*": "./dist/css/*.css",
    "./types/*": "./dist/types/*.ts",
    "./utilities/*": "./dist/utilities/*.ts"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/tide2-design-system.js",
  "name": "tide2-design-system",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build && cp -r src/assets/css/ dist/css/ && cp -r src/types/ dist/types/ && cp -r src/utilities/ dist/utilities/",
    "build-storybook": "storybook build",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore --fix",
    "sb": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "type-check": "vue-tsc --noEmit"
  },
  "type": "module",
  "version": "0.2.8"
}
